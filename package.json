    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.17.3",
    "chromedriver": "^2.29.0",
    "del": "^2.0.2",
    "gh-pages": "^0.12.0",
    "gulp": "^3.9.1",
